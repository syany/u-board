<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.shape.Rectangle?>

<fx:root
  type="javafx.scene.layout.VBox"
  prefWidth="1240.0" prefHeight="400.0"
  style="-fx-background-color: #ccc;"
  xmlns:fx="http://javafx.com/fxml">
  <children>
    <HBox id="ControllArea">
      <children>
        <Label id="SynthesizerLabel" styleClass="comLabel" text="出力:" />
        <ComboBox id="SynthesizerSelect"></ComboBox>
        <Label styleClass="comLabel" text=" " />
        <Label id="SequencerLabel" styleClass="comLabel" text="入力:" />
        <ComboBox id="SequencerSelect"></ComboBox>
        <Label styleClass="comLabel" text=" " />
        <Button id="deviceButton">デバイス設定</Button>
        <Label styleClass="comLabel" text=" " />
        <Button id="keySetButton">キー設定</Button>
        <Label id="commentabel" styleClass="titleLabel"
          text="U-BOARD 1"
          prefWidth="1110.0"
        />
      </children>
    </HBox>
    <HBox id="KeyArea">
      <children>
        <VBox id="optionArea"
          style="-fx-alignment:center;"
        >
          <children>
          </children>
        </VBox>
      </children>
    </HBox>
  </children>
</fx:root>